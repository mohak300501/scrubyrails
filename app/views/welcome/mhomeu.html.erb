<%= form_tag '../announcement/' + @news.id.to_s do %>
    <div>
        <%= label_tag "title" %><br/>
        <%= text_field_tag :name, @news.news, :required => true, class: "mtextfield" %><br/>
        
        
    </div>
    <div>
        <%= submit_tag "अग्रेस्थापनं करोतु", :class => "button" %><br/>
        <%= link_to "प्रतिगच्छतु", all_announcements_path %>
    </div>
<% end %>