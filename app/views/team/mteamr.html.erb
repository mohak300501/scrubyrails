<div class="button">
    <%= link_to "Back", member_area_path, class: "anone" %>
</div>

<% for i in @teams do %>
    <%= content_tag :div, class: "teams" do %>
        <div class="title">
            <%= i.name %><br/>
        </div>

        <div class="updel">
            <%= link_to team_path(i.id), class: "anone" do %>
                <div class="button">परिवर्तयतु</div>
            <% end %>
            <%= link_to team_delete_path(i.id), method: "delete", data: {:confirm => "Are you sure you want to delete this member?"}, class: "anone" do %>
                <div class="button">विनश्यतु</div>
            <% end %>
        </div>
    <% end %>
<% end %>