<div class="overf">
    <table class="feedback">
        <tr>
            <th>Email</th>
            <th>Type</th>
            <th>Feedback</th>
        </tr>
        <% for i in @feedbacks do %>
            <tr>
                <td>
                    <%= i.email %><br/>
                </td>
                <td>
                    <p><%= i.typef %></p>
                </td>
                <td>
                    <% for j in i.feedback.split("\n") do %>
                        <%= j %><br/>
                    <% end %>
                </td>
            <tr/>
        <% end %>
    </table>
</div>