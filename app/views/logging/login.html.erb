<h1>Log In</h1>
<%= form_tag do %>
    <div>
        <%= label_tag :email %><br/>
        <%= text_field_tag :email, type:'email', required: true, placeholder: "Email" %><br/>
        <%= label_tag :password %><br/>
        <%= password_field_tag :password, required: true, placeholder: "Password" %>
    </div>
    <div>
        <%= submit_tag "Log In" %>
    </div>
    <div>
        <%= @mssg %>
    </div>
<% end %>
